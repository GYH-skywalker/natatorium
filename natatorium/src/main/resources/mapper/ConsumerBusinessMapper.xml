        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.natatorium.mapper.ConsumerBusinessMapper">

    <resultMap id="BaseResultMap" type="com.example.natatorium.entity.ConsumerBusiness">
